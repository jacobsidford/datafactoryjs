{
	"name": "datafactoryjs",
	"version": "1.0.0",
	"description": "",
	"main": "src/index.js",
	"files": [
		"src/"
	],
	"scripts": {
		"test": "jest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jacobsidford/datafactoryjs.git"
	},
	"keywords": [
		"testing",
		"mock",
		"test data",
		"factories"
	],
	"prettier": {
		"useTabs": true,
		"singleQuote": true,
		"trailingComma": "none",
		"bracketSpacing": true,
		"semi": true,
		"arrowParens": "always"
	},
	"jest": {
		"testEnvironment": "node",
		"testMatch": [
			"**/__tests__/**/*.[jt]s?(x)",
			"**/?(*.)+(spec|test).[tj]s?(x)"
		],
		"testPathIgnorePatterns": [
			"/node_modules/"
		]
	},
	"author": "Jacob Sidford",
	"license": "MIT",
	"babel": {
		"presets": [
			"@babel/preset-env"
		],
		"plugins": [
			"@babel/proposal-class-properties"
		]
	},
	"bugs": {
		"url": "https://github.com/jacobsidford/datafactoryjs/issues"
	},
	"homepage": "https://github.com/jacobsidford/datafactoryjs#readme",
	"devDependencies": {
		"@babel/core": "^7.10.2",
		"@babel/node": "^7.10.1",
		"@babel/plugin-proposal-class-properties": "^7.10.1",
		"@babel/preset-env": "^7.10.2",
		"jest": "^26.0.1",
		"prettier": "^2.0.5"
	}
}
